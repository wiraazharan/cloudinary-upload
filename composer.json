{
    "name": "wiraazharan/cloudinary-upload",
    "description": "This package is built on top of intervention and php-cloudinary to ease image upload process",
    "type": "library",
    "require": {
        "intervention/image": "^2.4",
        "cloudinary/cloudinary_php": "^1.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "WIRA AZHARAN AKMAR",
            "email": "wiraazharan@gmail.com"
        }
    ],
    "autoload" : {
      "psr-4": {
        "Wiraazharan\\Cloudinaryupload\\": "vendor/wiraazharan/cloudinary-upload/src"
      }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Wiraazharan\\Cloudinaryupload\\UploadServiceProvider"
            ],
            "aliases": {
                "CloudinaryUpload": "Wiraazharan\\Cloudinaryupload\\UploadFacade"
            }
        }
    }
}
